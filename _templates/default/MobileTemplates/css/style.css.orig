.point-Kansai{position:absolute;top:205px;left:134px}
.area-chu{width:14%;fill:#2D4452;position:absolute;left:104px;top:154px}
.point-Chu{position:absolute;top:126px;left:35px}
.area-kyu{width:18%;fill:#039ACD;position:absolute;left:46px;top:159px}
.point-Kyu{position:absolute;left:0;top:213px}
.area-kyu .st1{fill:none;stroke:#E4EDD6;stroke-width:2;stroke-miterlimit:10}
.area-hok:hover{fill:#737574;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-tokai:hover{fill:#23965c;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-kanto:hover{fill:#8c030e;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-kansai:hover{fill:#390259;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-chu:hover{fill:#0e212b;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-kyu:hover{fill:#03576d;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.btn-point, .btn-points {font-size:10px;background:#fff;border-radius:10px;webkit-border-radius:10px;display:inline-block;padding:3px 10px;border:1px solid #ccc;position:absolute}
.btn-point:hover,.btn-point:focus, .btn-points:hover,.btn-points:focus{text-decoration:none}
.area-americas{width:40%;fill-rule:evenodd;clip-rule:evenodd;fill:#E2574C;position:absolute;left:-24px;top:50px}
.area-americas:hover{fill:#aa241d;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-european{width:40%;fill:#878787;position:absolute;left:82px;top:52px}
.area-european:hover{fill:#494949;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-asia{width:40%;fill-rule:evenodd;clip-rule:evenodd;fill:#EB6D20;position:absolute;left:124px;top:65px}
.area-asia:hover{fill:#9b3700;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-africa{width:27%;fill-rule:evenodd;clip-rule:evenodd;fill:#2D4452;position:absolute;left:67px;top:89px}
.area-africa:hover{fill:#142933;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.area-ocean{width:30%;fill:#7DE2B2;position:absolute;right:1px;top:135px}
.area-ocean:hover{fill:#319965;cursor:pointer;-webkit-transition:-webkit-transform .6s ease-out;transition:transform .6s ease-out}
.point-Europe{top:20px;right:113px}
.point-Asia{top:71px;right:86px}
.point-Ocean{top:180px;right:50px}
.point-Africa{top:175px;left:90px}
.point-Americas{top:30px;left:0}

