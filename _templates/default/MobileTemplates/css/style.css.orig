@import "http://fonts.googleapis.com/css?family=Open+Sans:400,700";
html,body{padding:0;margin:0}
body{background-color:#fff;font-family:'Open Sans',Arial,sans-serif;font-size:14px;line-height:1.3em;color:#bababa;position:relative;-webkit-text-size-adjust:none}
body *{text-shadow:none}
h1,h2,h3,h4,h5,h6{line-height:1;font-weight:700;margin:20px 0 10px;font-family:'Open Sans',Arial,sans-serif;font-family:'Open Sans',sans-serif; line-height: 1.5em;}
h1,h2{font-size:16px; margin-top: 13px;}
h3{font-size:14px;}
h4,h5,h6{font-size:12px; margin-top: 0;}
h2 {margin-top: 0; padding: 15px 0; margin-bottom: 0;}
h4 {font-weight: normal;font-size: 14px;}
p{margin:0 0 10px}
a,a:link,a:active,a:visited,a:hover{color:inherit;color:#318c6d}
nav:not(.mm-menu){display:none}
.header,.content,.footer,.header2{text-align:center}

.header{background:#fff;font-size:16px;font-weight:700;color:#fff;line-height:40px;-moz-box-sizing:border-box;box-sizing:border-box;width:100%;height:40px;padding:0 50px}
.footer.fixed{position:fixed;bottom:0;left:0}
.header a.menu,.header2 a.menu{background:#7de2b0;display:block;width:40px;height:40px;position:absolute;top:30px;right:10px;color:#fff;-webkit-border-radius:50%;border-radius:50%}
.header a.menu:hover,.header2 a.menu:hover{background:#298776}
.content{padding:0;color:#454040;text-align:left}


/*****Nicker****/
