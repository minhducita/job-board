.map-japan,.map-world{position:relative}
.map-world{display:none}
.w-optionMap{}
.area-hok{fill:#ccc;width:20%;position:absolute;top:0;right:0}
.point-Hok{position:absolute;top:40px;right:58px}
.area-tokai{width:23%;fill:#7DE2B2;position:absolute;right:52px;top:105px}
.point-Tok{position:absolute;top:100px;right:70px}
.area-kanto{width:16%;fill:#D3222A;position:absolute;right:21px;top:127px}
.point-Kanto{position:absolute;top:175px;right:8px}
